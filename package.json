{
    "name": "test_client_commons",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "scripts": {
        "build:components": "tsup src/components/index.ts --format esm,cjs --dts --out-dir dist/components",
        "build:types": "tsup src/types/index.ts --format esm,cjs --dts --out-dir dist/types",
        "build": "npm run build:components && npm run build:types && tsup src/index.ts --format esm,cjs --dts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "exceljs": "^4.4.0",
        "file-saver": "^2.0.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@swc/core": "^1.7.36",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.1",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.47",
        "tailwindcss": "^3.4.14",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./components": {
            "import": "./dist/components/index.mjs",
            "require": "./dist/components/index.js"
        },
        "./types": {
            "import": "./dist/types/index.mjs",
            "require": "./dist/types/index.js"
        }
    },
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "components": [
                "./dist/components/index.d.ts"
            ],
            "types": [
                "./dist/types/index.d.ts"
            ]
        }
    }
}
